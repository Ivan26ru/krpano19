<!-- Оптимальные настройки для навигатора -->
<plugin
	name="navigator"
	keep="true"
	devices="html5"
	url="%VIEWER%/plugins/navigator.js"
	leftkey="65"
	rightkey="68"
	forwardkey="87"
	backwardkey="83"
	upkey="81"
	downkey="69"
	angletolerance="60"
	hotspotvisibility="model"
	notfoundmove="10"
	notfoundtime="0.3"
	navigationmode="walking"
	navigationclick="true"
	navigationspeed="80"
	walkingspeed="0.2"
	walkingfriction="0.9"
	walkratiochange="0.8"
	walktransition="true"
	loadsceneflags="MERGE|KEEPVIEW|KEEPMOVING"
	loadscenevars="null"
	loadsceneblend="BLEND"
	loadsceneblendtime="0.2"
	modereturn="false"
	returnaction="null"
	depthmapuse="true"
	hapticfeedback="true"
	editmode="true"
/>